[user]
	email = jeffrey.mitchell@gmail.com
	name = Jeff Mitchell
[credential]
	helper = store
[push]
	default = upstream
[core]
	editor = nvim
[alias]
    pr = "!f() { git fetch origin pull/$1/head:pr-$1 && git checkout pr-$1;  }; f"
    top = !git log --format=format:%an | sort | uniq -c | sort -r | head -n 20
[rerere]
	enabled = true
[http "https://gopkg.in"]
	followRedirects = true
